{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "coreclr",
            "request": "launch",
            "symbolOptions": {
                "searchPaths": ["${workspaceFolder}/LDtkMonogame.Examples/obj/Debug/net5.0/"],
                "searchMicrosoftSymbolServer": true,
                "searchNuGetOrgSymbolServer": false
            },
            "preLaunchTask": "build",
            "justMyCode": true,
            "suppressJITOptimizations": true,
            "program": "${workspaceFolder}/LDtkMonogame.Examples/bin/Debug/net5.0/LDtkMonogame.Examples.dll",
            "args": [],
            "cwd": "${workspaceFolder}/LDtkMonogame.Examples",
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}